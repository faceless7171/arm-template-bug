{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "type": "string",
      "defaultValue": "[resourceGroup().location]",
      "metadata": {
        "description": "Location for all resources."
      }
    }
  },
  "variables": {},
  "resources": [
    {
      "name": "test-bug-storage",
      "type": "Microsoft.Storage/storageAccounts",
      "apiVersion": "2018-07-01",
      "location": "[parameters('location')]",
      "kind": "Storage",
      "sku": {
        "name": "Standard_RAGRS"
      },
      "properties": {
      }
    },
    {
      "name": "test-bug-service-plan",
      "type": "Microsoft.Web/serverfarms",
      "apiVersion": "2018-02-01",
      "kind": "functionapp",
      "location": "[parameters('location')]",
      "tags": {},
      "properties": {
        "name": "test-bug-service-plan",
        "perSiteScaling": false,
        "reserved": false,
        "targetWorkerCount": 0,
        "targetWorkerSizeId": 0
      },
      "sku": {
        "name": "Y1",
        "tier": "Dynamic",
        "size": "Y1",
        "family": "Y",
        "capacity": 0
      }
    },
    {
      "name": "test-bug-func",
      "type": "Microsoft.Web/sites",
      "apiVersion": "2018-02-01",
      "kind": "functionapp",
      "location": "[parameters('location')]",
      "properties": {
        "serverFarmId": "[resourceId('Microsoft.Web/serverfarms', 'test-bug-service-plan')]",
        "reserved": false,
        "siteConfig": {
          "use32BitWorkerProcess": false,
          "appSettings": [
            {
              "name": "TestKey",
              "value": "[listKeys(resourceId('Microsoft.Storage/storageAccounts', 'test-bug-storage'),  providers('Microsoft.Storage', 'storageAccounts').apiVersions[0]).keys[0].value]"
            }
          ]
        },
        "clientAffinityEnabled": false,
        "clientCertEnabled": false,
        "hostNamesDisabled": false,
        "containerSize": 1536,
        "dailyMemoryTimeQuota": 0,
        "httpsOnly": false
      },
      "resources": [
      ],
      "dependsOn": [
        "[resourceId('Microsoft.Web/serverfarms', 'test-bug-service-plan')]",
        "[resourceId('Microsoft.Storage/storageAccounts', 'test-bug-storage')]"
      ]
    }
  ],
  "outputs": {
  }
}
